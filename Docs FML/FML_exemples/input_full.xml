	\vce=speaker=Antoine\ \rspd=100\ \vol=26214\ \vct=100\
	Voilà bien longtemps,
	<tm id="tm2"/>
	\pau=350\
	un soir de printemps,
	<tm id="tm3"/>
	\pau=350\
	trois petits morceaux de nuit se détachèrent du ciel 
	<tm id="tm4"/>
	\pau=250\
	et tombèrent sur Terre.
	<tm id="tm5"/>
	\pau=350\
	Ils étaient 
	<tm id="tm6"/>
	\rspd=94\
	tout noirs
	\rspd=100\
	<tm id="tm7"/>
	,
	<tm id="tm8"/>
	\rspd=94\
